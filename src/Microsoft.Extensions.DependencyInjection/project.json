{
  "version": "1.1.1",
  "description": "Default implementation of dependency injection for Microsoft.Extensions.DependencyInjection.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/dotnet40/dependencyinjection"
    },
    "tags": [
      "dependencyinjection",
      "di"
    ]
  },
  "buildOptions": {
    "compile": {
      "includeFiles": [ "../ReflectionExtensions.cs", "../ExceptionDispatchInfo.cs" ]
    },
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.1.1"
  },
  "frameworks": {
    "net40": {
      "dependencies": {
      }
    }
  }
}